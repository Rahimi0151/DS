ex10 (L, ref L1, ref L2 : list):
{
    if L == NULL :
        return
    while L^.next !=  NULL:
    {
        L1^.next = L^.next
        L1 = L1^.next
        L = L^.next
        if L != NULL:
        {
            L2^.next = L^.next
            L = L^.next
            L = L^.next
            L2 = L2^.next
        }
    }
}


